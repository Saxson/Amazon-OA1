class CNode {
    int val;
    CNode next;
    public CNode(int val){
        this.val = val;
        this.next = null;
    }
}
public class InsertToCycle {
    public CNode insert(CNode head, int x){
        if (head == null){
            head = new CNode(x);
            head.next = head;
            return head;
        }
        CNode cur = head;
        CNode pre;
        do{
            pre = cur;
            cur = cur.next; 
            if (x <= cur.val && x >= pre.val){
                break;
            }
            if (pre.val > cur.val && (x < cur.val || x > pre.val)){
                break;
            }
        }while(cur != head);
        CNode newNode = new CNode(x);
        newNode.next = cur;
        pre.next = newNode;
        return newNode;
    }
